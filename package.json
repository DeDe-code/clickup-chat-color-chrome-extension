{
  "name": "umc-chrome-extention",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node build-utils/build-tailwind.js && vite",
    "build": "node build-utils/build-tailwind.js && vite build && node build-utils/post-build.js",
    "build:tailwind": "node build-utils/build-tailwind.js",
    "preview": "vite preview",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "dev:ext": "concurrently \"npm run dev:watch\" \"npm run dev:serve\"",
    "dev:watch": "node build-utils/build-tailwind.js && vite build --watch && node build-utils/post-build.js",
    "dev:serve": "web-ext run --source-dir=./dist --target=chromium",
    "build:ext": "npm run build && web-ext build --source-dir=./dist --artifacts-dir=./web-ext-artifacts --overwrite-dest",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "pinia": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-colorpicker": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite": "^6.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "web-ext": "^8.8.0"
  }
}
